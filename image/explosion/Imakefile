/*
 * common part 
 */

XBLASTDIR=$(LIBDIR)/xblast
IMAGEDIR=$(XBLASTDIR)/image/explosion

/*
 * install rule	
 */

PPMFILES=*.ppm
PBMFILES=*.pbm
ALLFILES=$(PPMFILES) $(PBMFILES)

InstallMultiple($(ALLFILES),$(IMAGEDIR))
